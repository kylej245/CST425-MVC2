                <package-root>test</package-root>
                <classpath mode="compile">/Users/kylej/apache-tomcat-7.0.30-8080/lib/servlet-api.jar:/Applications/eclipse/plugins/org.junit_4.10.0.v4_10_0_v20120426-0900/junit.jar</classpath>
