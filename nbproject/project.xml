            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">/Users/kylej/apache-tomcat-7.0.30-8080/lib/servlet-api.jar</classpath>
                <source-level>1.6</source-level>
            </compilation-unit>
        </java-data>
        <web-data xmlns="http://www.netbeans.org/ns/freeform-project-web/2">
            <web-module>
                <doc-root>.</doc-root>
                <classpath/>
                <context-path/>
                <j2ee-spec-level>1.5</j2ee-spec-level>
                <web-inf>build/WEB-INF</web-inf>
            </web-module>
        </web-data>
    </configuration>
